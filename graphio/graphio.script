/create DIRECTED
/addvertex A
/addvertex B
/addvertex C

# Serialize/Deserialize
/write d-abc.graph
/read d-abc.graph

/vertexcount 3
/containsvertex A YES
/containsvertex B YES
/containsvertex C YES
/degree A 0
/degree B 0
/degree C 0

